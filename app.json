{
  "expo": {
    "name": "K-Cashless-app",
    "slug": "K-Cashless-app",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.k-cashless.k-cashless-alpha",
      "buildNumber": "0.1.0"
    },
    "android": {
      "package": "com.kcashless.kcashless_alpha",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "VIBRATE"
      ]
    }
  }
}
